<h1>Editing Book</h1>
<%= form_with model: @list do |f| %>

<div class="field">
  <table for="book_title">本のタイトル</table>
  <%= f.text_field :title %>
</div>

<div class="field">
  <table for="book_body">感想</table>
  <%= f.text_area :body %>
</div>

<div class="field">
  <%= f.submit 'Update Book' %>
</div>
<% end %>

<%= link_to "Show", book_path(@book.id) %>
<%= "|" %>
<%= link_to "Back", books_path %>